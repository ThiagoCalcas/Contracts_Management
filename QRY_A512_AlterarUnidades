SELECT TBL_Unidades.CodUnidades, TBL_Unidades.CodigoSISFIN, TBL_Unidades.NomeUnidade, TBL_Unidades.Endereco, TBL_Unidades.Bairro, TBL_Unidades.CEP, TBL_Unidades.CodRoteiros, TBL_Unidades.CodAditamentos, TBL_Unidades.SupRotineiro, TBL_Unidades.SupEventual, TBL_Unidades.SupEspecial, TBL_Unidades.RecRotineiro, TBL_Unidades.RecEventual, TBL_Unidades.RecEspecial, TBL_Unidades.Conjugado, TBL_Unidades.AdValoremBB, TBL_Unidades.AdValoremOutros, TBL_Unidades.Custodia, TBL_Unidades.TratCedulas, TBL_Unidades.TratMoedas, TBL_Unidades.SupRotMult, TBL_Unidades.SupEveMult AS [Adicional/SupEveMult], TBL_Unidades.SupEspMult, TBL_Unidades.AcionEveSimples, TBL_Unidades.AcionEspSimples, TBL_Unidades.AcionComplementarSimples, TBL_Unidades.AcionEveMultiplo, TBL_Unidades.AcionEspMultiplo, TBL_Unidades.AcionComplementarMultiplo, TBL_Unidades.CarroDedicado, TBL_Unidades.MotoDedicada
FROM (TBL_Aditamentos INNER JOIN TBL_Unidades ON TBL_Aditamentos.CodAditamentos = TBL_Unidades.CodAditamentos) INNER JOIN TBL_Roteiros ON TBL_Unidades.CodRoteiros = TBL_Roteiros.CodRoteiros
WHERE (((TBL_Aditamentos.CodContratos)=[Forms]![FRM_Alteracao_5_0CdC_Selecionar]![cbo_NomeContrato]))
ORDER BY TBL_Roteiros.[Municipio / Roteiro];
