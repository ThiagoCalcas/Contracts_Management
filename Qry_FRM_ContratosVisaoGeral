Round([SaldoContrato2]/[Media_5-1_Meses],2) AS [Estimativa5-1Meses2],
QRY__PagamentoPorContrato.SomaDeValorTotal3,
[ValorGlobal3]+[ValorAdit3] AS ValorTotalContrato3,
[ValorTotalContrato3]-[SomaDeValorTotal3] AS SaldoContrato3, 
Round([SaldoContrato3]/[ValorMensalPorContrato],2) AS EstimativaDuracao3, 
Round([SaldoContrato3]/[Media_2_Meses],2) AS Estimativa2Meses3,
Round([SaldoContrato3]/[Media_3-5_Meses],2) AS EstimativaDuracaoMedia3, 
Round([SaldoContrato3]/[Media_5_Meses],2) AS Estimativa5Meses3, 
Round([SaldoContrato3]/[Media_5-1_Meses],2) AS [Estimativa5-1Meses3], 
QRY__PagamentoPorContrato.SomaDeValorTotal4, 
[ValorGlobal4]+[ValorAdit4] AS ValorTotalContrato4, 
[ValorTotalContrato4]-[SomaDeValorTotal4] AS SaldoContrato4, 
Round([SaldoContrato4]/[ValorMensalPorContrato],2) AS EstimativaDuracao4,
Round([SaldoContrato4]/[Media_2_Meses],2) AS Estimativa2Meses4,
Round([SaldoContrato4]/[Media_3-5_Meses],2) AS EstimativaDuracaoMedia4,
Round([SaldoContrato4]/[Media_5_Meses],2) AS Estimativa5Meses4, 
Round([SaldoContrato4]/[Media_5-1_Meses],2) AS [Estimativa5-1Meses4], 
QRY__PagamentoPorContrato.SomaDeValorTotal5, 
[ValorGlobal5]+[ValorAdit5] AS ValorTotalContrato5, 
[ValorTotalContrato5]-[SomaDeValorTotal5] AS SaldoContrato5, 
Round([SaldoContrato5]/[ValorMensalPorContrato],2) AS EstimativaDuracao5, 
Round([SaldoContrato5]/[Media_2_Meses],2) AS Estimativa2Meses5, 
Round([SaldoContrato5]/[Media_3-5_Meses],2) AS EstimativaDuracaoMedia5,
Round([SaldoContrato5]/[Media_5_Meses],2) AS Estimativa5Meses5, 
Round([SaldoContrato5]/[Media_5-1_Meses],2) AS [Estimativa5-1Meses5], 
QRY__PagamentoPorContrato.SomaDeValorTotal6, 
[ValorGlobal6]+[ValorAdit6] AS ValorTotalContrato6, 
[ValorTotalContrato6]-[SomaDeValorTotal6] AS SaldoContrato6,
Round([SaldoContrato6]/[ValorMensalPorContrato],2) AS EstimativaDuracao6,
Round([SaldoContrato6]/[Media_2_Meses],2) AS Estimativa2Meses6, 
Round([SaldoContrato6]/[Media_3-5_Meses],2) AS EstimativaDuracaoMedia6, 
Round([SaldoContrato6]/[Media_5_Meses],2) AS Estimativa5Meses6, 
Round([SaldoContrato6]/[Media_5-1_Meses],2) AS [Estimativa5-1Meses6], 
QRY__PagamentoPorContrato.SomaDeValorTotal7, 
[ValorGlobal7]+[ValorAdit7] AS ValorTotalContrato7,
[ValorTotalContrato7]-[SomaDeValorTotal7] AS SaldoContrato7, 
Round([SaldoContrato7]/[ValorMensalPorContrato],2) AS EstimativaDuracao7,
Round([SaldoContrato7]/[Media_2_Meses],2) AS Estimativa2Meses7, 
Round([SaldoContrato7]/[Media_3-5_Meses],2) AS EstimativaDuracaoMedia7,
Round([SaldoContrato7]/[Media_5_Meses],2) AS Estimativa5Meses7, 
Round([SaldoContrato7]/[Media_5-1_Meses],2) AS [Estimativa5-1Meses7], 
QRY__PagamentoPorContrato.SomaDeValorTotal8, 
[ValorGlobal8]+[ValorAdit8] AS ValorTotalContrato8,
[ValorTotalContrato8]-[SomaDeValorTotal8] AS SaldoContrato8, 
Round([SaldoContrato8]/[ValorMensalPorContrato],2) AS EstimativaDuracao8,
Round([SaldoContrato8]/[Media_2_Meses],2) AS Estimativa2Meses8, 
Round([SaldoContrato8]/[Media_3-5_Meses],2) AS EstimativaDuracaoMedia8, 
Round([SaldoContrato8]/[Media_5_Meses],2) AS Estimativa5Meses8, 
Round([SaldoContrato8]/[Media_5-1_Meses],2) AS [Estimativa5-1Meses8], 
QRY_ConsultaContratos_SQL.ValorMensalPorContrato, 
QRY__PagamentoMaisRecente.MesUltimoPgto0, 
QRY__PagamentoMaisRecente.AnoUltimoPgto0, 
QRY__PagamentoMaisRecente.MesUltimoPgto1, 
QRY__PagamentoMaisRecente.AnoUltimoPgto1, 
QRY__PagamentoMaisRecente.MesUltimoPgto2, 
QRY__PagamentoMaisRecente.AnoUltimoPgto2, 
QRY__PagamentoMaisRecente.MesUltimoPgto3, 
QRY__PagamentoMaisRecente.AnoUltimoPgto3, 
QRY__PagamentoMaisRecente.MesUltimoPgto4, 
QRY__PagamentoMaisRecente.AnoUltimoPgto4, 
QRY__PagamentoMaisRecente.MesUltimoPgto5, 
QRY__PagamentoMaisRecente.AnoUltimoPgto5, 
QRY__PagamentoMaisRecente.MesUltimoPgto6, 
QRY__PagamentoMaisRecente.AnoUltimoPgto6, 
QRY__PagamentoMaisRecente.MesUltimoPgto7, 
QRY__PagamentoMaisRecente.AnoUltimoPgto7, 
QRY__PagamentoMaisRecente.MesUltimoPgto8, 
QRY__PagamentoMaisRecente.AnoUltimoPgto8, 
IIf([AnoUltimoPgto0]="0",(Year([FimContratado])-Year([InicioContratado]))*12+Month([FimContratado])-Month([InicioContratado]),(Year([FimContratado])-[AnoUltimoPgto0])*12+Month([FimContratado])-[MesUltimoPgto0]) AS PgtosRestantesVig0, 
IIf([SomaDeValorTotal1]=0,(Year([FimProrrogacao1])-Year([InicioProrrogacao1]))*12+Month([FimProrrogacao1])-Month([InicioProrrogacao1]),(Year([FimProrrogacao1])-[AnoUltimoPgto1])*12+Month([FimProrrogacao1])-[MesUltimoPgto1]) AS PgtosRestantesVig1,
IIf([SomaDeValorTotal2]=0,(Year([FimProrrogacao2])-Year([InicioProrrogacao2]))*12+Month([FimProrrogacao2])-Month([InicioProrrogacao2]),(Year([FimProrrogacao2])-[AnoUltimoPgto2])*12+Month([FimProrrogacao2])-[MesUltimoPgto2]) AS PgtosRestantesVig2,
IIf([SomaDeValorTotal3]=0,(Year([FimProrrogacao3])-Year([InicioProrrogacao3]))*12+Month([FimProrrogacao3])-Month([InicioProrrogacao3]),(Year([FimProrrogacao3])-[AnoUltimoPgto3])*12+Month([FimProrrogacao3])-[MesUltimoPgto3]) AS PgtosRestantesVig3,
IIf([SomaDeValorTotal4]=0,(Year([FimProrrogacao4])-Year([InicioProrrogacao4]))*12+Month([FimProrrogacao4])-Month([InicioProrrogacao4]),(Year([FimProrrogacao4])-[AnoUltimoPgto4])*12+Month([FimProrrogacao4])-[MesUltimoPgto4]) AS PgtosRestantesVig4,
IIf([SomaDeValorTotal5]=0,(Year([FimProrrogacao5])-Year([InicioProrrogacao5]))*12+Month([FimProrrogacao5])-Month([InicioProrrogacao5]),(Year([FimProrrogacao5])-[AnoUltimoPgto5])*12+Month([FimProrrogacao5])-[MesUltimoPgto5]) AS PgtosRestantesVig5,
IIf([SomaDeValorTotal6]=0,(Year([FimProrrogacao6])-Year([InicioProrrogacao6]))*12+Month([FimProrrogacao6])-Month([InicioProrrogacao6]),(Year([FimProrrogacao6])-[AnoUltimoPgto6])*12+Month([FimProrrogacao6])-[MesUltimoPgto6]) AS PgtosRestantesVig6,
IIf([SomaDeValorTotal7]=0,(Year([FimProrrogacao7])-Year([InicioProrrogacao7]))*12+Month([FimProrrogacao7])-Month([InicioProrrogacao7]),(Year([FimProrrogacao7])-[AnoUltimoPgto7])*12+Month([FimProrrogacao7])-[MesUltimoPgto7]) AS PgtosRestantesVig7,
IIf([SomaDeValorTotal8]=0,(Year([FimProrrogacao8])-Year([InicioProrrogacao8]))*12+Month([FimProrrogacao8])-Month([InicioProrrogacao8]),(Year([FimProrrogacao8])-[AnoUltimoPgto8])*12+Month([FimProrrogacao8])-[MesUltimoPgto8]) AS PgtosRestantesVig8,
QRY_MediaPgto.Media_5_Meses, 
QRY_MediaPgto.[Media_5-1_Meses], 
QRY_MediaPgto.Media_2_Meses, 
QRY_MediaPgto2.[Media_3-5_Meses]

FROM 
(((TBL_GILOG RIGHT JOIN (((QRY_ConsultaContratos_SQL LEFT JOIN QRY__PagamentoPorContrato ON QRY_ConsultaContratos_SQL.CodContratos = QRY__PagamentoPorContrato.CodContratos) INNER JOIN TBL_Contratos ON QRY_ConsultaContratos_SQL.CodContratos = TBL_Contratos.CodContratos) LEFT JOIN QRY__PagamentoMaisRecente ON TBL_Contratos.CodContratos = QRY__PagamentoMaisRecente.CodContratos) ON TBL_GILOG.CodGILOG = TBL_Contratos.CodGILOG) INNER JOIN QRY_CalcAditMensal ON TBL_Contratos.CodContratos = QRY_CalcAditMensal.CodContratos) LEFT JOIN QRY_MediaPgto ON TBL_Contratos.CodContratos = QRY_MediaPgto.CodContratos) LEFT JOIN QRY_MediaPgto2 ON TBL_Contratos.CodContratos = QRY_MediaPgto2.CodContratos

WHERE 
(((QRY_ConsultaContratos_SQL.Situacao)="Ativo"))

ORDER BY 
QRY_ConsultaContratos_SQL.UF,
QRY_ConsultaContratos_SQL.UF, 
QRY_ConsultaContratos_SQL.Tipo, 
QRY_ConsultaContratos_SQL.CodContratos;
